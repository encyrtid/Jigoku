<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Jigoku.Core.Entities" assembly="Jigoku.Core">
	<class name="PrivateMessage" table="PrivateMessage">
		<id column="Id" name="Id">
			<generator class="sequence">
				<param name="sequence">PrivateMessage_Id_seq</param>
			</generator>
		</id>
		<property name="Topic" column="Topic" type="String"/>
		<property name="Body" column="Body" type="String"/>
		<property name="Attachment" column="Attachment" type="byte[]"/>
		<property name="DateSend" column="DateSend" type="DateTime" not-null="true"/>
		<property name="DateReceive" column="DateReceive" type="DateTime" not-null="true"/>
		<property name="IdTo" column="IdTo" not-null="true"/>
	</class>
</hibernate-mapping>