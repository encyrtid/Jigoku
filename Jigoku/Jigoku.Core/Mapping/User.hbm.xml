<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Jigoku.Core" namespace="Jigoku.Core.Entities">
	<class name="User" table="users">
		<id name="Uid">
			<column name="Uid"/>
			<generator class="sequence">
				<param name="Users_Uid_seq"/>
			</generator>
		</id>
		<property name="NickName" type="String" not-null="true"/>
		<property name="Password" type="String" not-null="true"/>
		<property name="PrimaryMail" type="String" column="email" not-null="true"/>
		<property name="UserPhoto"/>
		<list name="Contacts" cascade="all" inverse="true">
			<key column="UserId" on-delete="cascade"/>
			<index column="Id"/>
			<one-to-many class="Contacts" />
		</list>
	</class>
</hibernate-mapping>